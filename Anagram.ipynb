    "# first we take function with two argument\n",
    "def Anagram(str1,str2):\n",
    "    if sorted(str1)==sorted(str2):\n",
    "        print(\"str1 and str2 is anagram\")\n",
    "    else:\n",
    "        print(\"not anagram\")\n",
    "\n",
    "str1= \"pappa\"\n",
    "str2 = \"ppapa\"\n",
    "\n",
    "Anagram(str1,str2)\n"
